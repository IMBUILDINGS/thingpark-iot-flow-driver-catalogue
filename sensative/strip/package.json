      "battery": {
        "type": "int64"
      },
      "proximity": {
        "type": "int64"
      },
      "presence": {
        "type": "boolean"
      },
      "closeProximity": {
        "type": "int64"
      },
      "closeProximityAlarm": {
        "type": "boolean"
      },
      "disinfectAlarm": {
        "type": "int64"
      }
    }
  },
  "devDependencies": {
    "jest": "^25.4.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
